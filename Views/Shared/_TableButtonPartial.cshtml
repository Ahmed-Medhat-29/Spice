@model int

<div class="btn-group text-white">
	<a class="btn btn-primary" asp-action="Edit" asp-route-Id="@Model">
		<i class="fas fa-edit"></i>
	</a>
	<a class="btn btn-success" asp-action="Details" asp-route-Id="@Model">
		<i class="fas fa-list-alt"></i>
	</a>
	<a class="btn btn-danger" asp-action="Delete" asp-route-Id="@Model">
		<i class="fas fa-trash-alt"></i>
	</a>
</div>